{
  "name": "weather-model",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.develop react-scripts start",
    "build": "env-cmd -f .env.develop react-scripts build",
    "test": "react-scripts test",
    "test:cover": "react-scripts test --coverage",
    "make-badges": "istanbul-badges-readme",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "prettier:fix": "prettier .eslintrc.js --write src/**/*.{js,jsx}",
    "release": "standard-version"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "json-summary"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test:cover && npm run make-badges && git add 'README.md'",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.21.0",
    "axios-extensions": "^3.1.3",
    "leaflet": "^1.7.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-leaflet": "^3.0.0-rc.0",
    "react-scripts": "4.0.0",
    "web-vitals": "^0.2.4",
    "zustand": "^3.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.1.10",
    "env-cmd": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-secrets": "^0.6.8",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.3.0",
    "istanbul-badges-readme": "^1.0.5",
    "prettier": "2.1.2",
    "react-test-renderer": "^17.0.1",
    "standard-version": "^9.0.0"
  }
}
